  "main": "./dist/index.js",
  "scripts": {
    "cy:ci": "start-server-and-test dev http://localhost:8080 cy:run",
    "cy:install": "npx cypress install",
    "cy:local": "start-server-and-test dev http://localhost:8080 cy:open",
    "cy:open": "npx cypress open",
    "cy:run": "npx cypress run",
    "dev": "NODE_ENV=development webpack serve --config webpack/webpack.dev.js",
    "lint": "eslint . --fix",
    "messages:extract": "formatjs extract 'src/**/*.{js,ts,tsx}' --out-file src/lang/data/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --format i18n/formatter.js",
    "messages:prettier": "npx prettier --write 'src/lang/data/*.json'",
    "production": "NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "test:watch": "npm run test -- --watch",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "author": "Pawel Szczepkowski",
